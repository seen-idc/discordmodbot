    },
    "unlock-channels": {
      "usage": "unlock-channels",
      "desc": "Unlocks all the channels in the server",
      "aliases": ["unlock-c", "ulgc"]
    },
    "reverify": {
      "usage": "reverify <@User>",
      "desc": "Runs the verification process again for the member"
    },
    "softban": {
      "usage": "softban <@User>",
      "desc": "Bans a member then immediately unbans them. (Basically a kick but deletes the messages)"
