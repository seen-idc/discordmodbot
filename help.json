    },
    "metrics": {
      "usage": "metrics",
      "desc": "Gets the system usage of the server that the bot is running on"
    },
    "shard-count": {
      "usage": "shard-count",
      "desc": "Gets the number of shards the bot has"
    },
    "purge": {
      "usage": "purge <FetchCount> \"contains\" <SearchString> <@User?>",
      "desc": "Deletes n number of messages"
