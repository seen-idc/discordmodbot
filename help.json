  "commands": {
    "ban": {
      "usage": "ban <User> <Reason?>",
      "desc": "Bans a member"
    },
    "kick": {
      "usage": "kick <User> <Reason?>",
      "desc": "Kicks a member"
    },
    "warn": {
      "usage": "warn <User> <Reason?>",
      "desc": "Warns a member",
      "aliases": ["w"]
    },
    "clear-warnings": {
      "usage": "clear-warnings <User>",
      "desc": "Clears a member's previous warnings",
