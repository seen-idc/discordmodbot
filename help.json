    "cat": {
      "usage": "cat",
      "desc": "Gets an image of a cute kitty"
    },
    "dog": {
      "usage": "dog",
      "desc": "Gets an image of a doggo"
    },
    "duck": {
      "usage": "duck",
      "desc": "Gets an image of a ducky"
    },
    "fox": {
      "usage": "fox",
      "desc": "Gets an image of a foxxy"
    },
